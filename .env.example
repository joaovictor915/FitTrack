# Configuração FitTrack

# Ambiente: development, testing, production
FLASK_ENV=development

# Banco de dados
DATABASE_URL=sqlite:///fittrack.db

# Chave secreta (mude em produção!)
SECRET_KEY=sua-chave-super-secreta-alterar-em-producao

# Configurações de sessão
SESSION_TYPE=filesystem
PERMANENT_SESSION_LIFETIME=86400

# Cors (adicione URLs conforme necessário)
CORS_ORIGINS=http://127.0.0.1:5500,http://localhost:5500,http://localhost:3000
